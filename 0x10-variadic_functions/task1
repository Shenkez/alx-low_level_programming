#include <stdio.h>
#include <stdarg.h>
#include "variadic_functions.h"

/**
 * int sum_them_all(const unsigned int n, ...) - check the code
 *
 * Return: Always 0.
 */
int sum_them_all(const unsigned int n, ...)
{
	var_list shenkezs;
	var_start(shenkezs, n);
	int count, i;
	sum=0;
	
	if n==0
		return (0);
	for(i = 0; i <= count; i++)
	{
		sum = sum + var_arg(shenkezs, int);
	}
	var_end(shenkezs);
	return (sum);

}
